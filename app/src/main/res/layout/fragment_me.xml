<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              >


<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="40dp">

    <Toolbar
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/baseBackground"
        >

    </Toolbar>

    <ImageView
        android:src="@drawable/set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="21dp"
        android:id="@+id/imageView"/>
</RelativeLayout>


    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/rl_my_1"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@drawable/bg_regist"
        >

        <ImageView
            android:id="@+id/iv_regi"
            android:src="@drawable/bg_my"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="18dp"/>

        <TextView
            android:id="@+id/tv_regist"
            android:text="用户登录"
            android:textSize="25dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/iv_regi"
            android:layout_marginStart="24dp"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_my_2"
        android:layout_width="match_parent"
        android:layout_height="40dp">

        <TextView
            android:text="我的订单"
            android:textSize="20dp"
            android:textStyle="bold"
            android:paddingLeft="10dp"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"/>

        <TextView
            android:id="@+id/tv_order"
            android:text="查看我的全部订单 >"
            android:textSize="15dp"
            android:gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView3"
            android:layout_alignBottom="@+id/textView3"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="16dp"/>

    </RelativeLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="90dp">

        <TextView
            android:id="@+id/tv_intention"
            android:text="意向中"
            android:textSize="15dp"
            android:drawableTop="@drawable/intention"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"/>

        <TextView
            android:background="@color/boundary"
            android:layout_width="2dp"
            android:layout_height="100dp"/>

        <TextView
            android:layout_weight="1"
            android:text="代付款"
            android:drawableTop="@drawable/pay"
            android:textSize="15dp"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_height="wrap_content"/>

        <TextView
            android:background="@color/boundary"
            android:layout_width="2dp"
            android:layout_height="100dp"/>

        <TextView

            android:text="待评价"
            android:drawableTop="@drawable/evalute"
            android:textSize="15dp"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"/>

    </LinearLayout>

    <TextView
        android:background="@color/boundary"
        android:layout_width="match_parent"
        android:layout_height="6dp"/>

    <RelativeLayout
        android:id="@+id/rl_my_3"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        >

        <ImageView
            android:src="@drawable/mypublish"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp"
            android:id="@+id/iv_my_publish"/>

        <TextView
            android:text="查看我发布的信息 >    "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            android:layout_alignBaseline="@+id/tv_my_publish"
            android:layout_alignBottom="@+id/tv_my_publish"
            android:layout_alignParentEnd="true"/>

        <TextView
            android:text="我的发布"
            android:textStyle="bold"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:id="@+id/tv_my_publish"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/iv_my_publish"/>

    </RelativeLayout>

    <TextView
        android:background="@color/boundary"
        android:layout_width="match_parent"
        android:layout_height="2dp"/>

    <RelativeLayout
        android:id="@+id/rl_my_4"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        >

        <TextView
            android:text="最爱经典 >    "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            android:layout_alignBaseline="@+id/tv_my_collect"
            android:layout_alignBottom="@+id/tv_my_collect"
            android:layout_alignParentEnd="true"/>

        <TextView
            android:text="我的收藏"
            android:textStyle="bold"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:id="@+id/tv_my_collect"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/iv_my_collect"/>

        <ImageView
            android:src="@drawable/mycollect"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="9dp"
            android:layout_centerVertical="true"
            android:id="@+id/iv_my_collect"
            android:layout_alignParentStart="true"/>

    </RelativeLayout>

    <TextView
        android:background="@color/boundary"
        android:layout_width="match_parent"
        android:layout_height="2dp"/>



    <RelativeLayout
        android:id="@+id/rl_my_5"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        >

        <TextView
            android:text="计划 >    "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            android:layout_alignBaseline="@+id/iv_my_plan"
            android:layout_alignBottom="@+id/iv_my_plan"
            android:layout_alignParentEnd="true"/>

        <TextView
            android:text="种植计划"
            android:textStyle="bold"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/iv_my_plan"/>

        <ImageView
            android:src="@drawable/myplan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="9dp"
            android:layout_centerVertical="true"
            android:id="@+id/iv_my_plan"
            android:layout_alignParentStart="true"/>

    </RelativeLayout>

    <TextView
        android:background="@color/boundary"
        android:layout_width="match_parent"
        android:layout_height="2dp"/>

    <RelativeLayout
        android:id="@+id/rl_my_6"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        >

        <ImageView
            android:src="@drawable/mychine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp"
            android:id="@+id/iv_my_chine"/>

        <TextView
            android:text="J;618小型农机 >    "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            android:layout_alignBaseline="@+id/iv_my_chine"
            android:layout_alignBottom="@+id/iv_my_chine"
            android:layout_alignParentEnd="true"/>

        <TextView
            android:text="我的农机"
            android:textStyle="bold"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/iv_my_chine"/>

    </RelativeLayout>

    <TextView
        android:background="@color/boundary"
        android:layout_width="match_parent"
        android:layout_height="6dp"/>

    <RelativeLayout
        android:id="@+id/rl_my_7"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        >

        <ImageView
            android:src="@drawable/mycard"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp"
            android:id="@+id/iv_my_card"/>

        <TextView
            android:text="每月优惠送不停 >    "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8"
            android:layout_alignBaseline="@+id/iv_my_card"
            android:layout_alignBottom="@+id/iv_my_card"
            android:layout_alignParentEnd="true"/>

        <TextView
            android:text="优惠卡卷"
            android:textStyle="bold"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/iv_my_card"/>

    </RelativeLayout>

    <TextView
        android:background="@color/boundary"
        android:layout_width="match_parent"
        android:layout_height="2dp"/>

    <RelativeLayout
        android:id="@+id/rl_my_8"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        >

        <ImageView
            android:src="@drawable/mynews"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp"
            android:id="@+id/iv_my_news"/>

        <TextView
            android:text="全场打折 >    "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView9"
            android:layout_alignBaseline="@+id/iv_my_news"
            android:layout_alignBottom="@+id/iv_my_news"
            android:layout_alignParentEnd="true"/>

        <TextView
            android:text="消息通知"
            android:textStyle="bold"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/iv_my_news"/>

    </RelativeLayout>


    <TextView
        android:background="@color/boundary"
        android:layout_width="match_parent"
        android:layout_height="2dp"/>

    <RelativeLayout
        android:id="@+id/rl_my_9"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        >

        <ImageView
            android:src="@drawable/myshare"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp"
            android:id="@+id/iv_my_share"/>

        <TextView
            android:text="帮朋友一起省钱 >    "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView0"
            android:layout_alignBaseline="@+id/iv_my_share"
            android:layout_alignBottom="@+id/iv_my_share"
            android:layout_alignParentEnd="true"/>

        <TextView
            android:text="好友分享"
            android:textStyle="bold"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/iv_my_share"/>

    </RelativeLayout>

    <TextView
        android:background="@color/boundary"
        android:layout_width="match_parent"
        android:layout_height="2dp"/>


    <RelativeLayout
        android:id="@+id/rl_my_10"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        >

        <ImageView
            android:src="@drawable/myabout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp"
            android:id="@+id/iv_my_about"/>

        <TextView
            android:text="帮朋友一起省钱 >    "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView1"
            android:layout_alignBaseline="@+id/iv_my_about"
            android:layout_alignBottom="@+id/iv_my_about"
            android:layout_alignParentEnd="true"/>

        <TextView
            android:text="好友分享"
            android:textStyle="bold"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/iv_my_about"/>

    </RelativeLayout>

        </LinearLayout>

    </ScrollView>

</LinearLayout>
